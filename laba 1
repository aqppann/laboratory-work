#include <cmath>
int main()
{
  int a = 4;
  float b = 300.1;
   float f;
   f = 7.2 * (a + b) * fabs(pow(((1 + pow(cos(a),2)) * (1 - cos(b))+ 0.711*log(a + b) ),3));
   cout<<"\n""result = "<<f;

// варіант 20
    int res1 = 1;
    float res2  = 1.201;
    float result;

    result = fabs(pow(0.7 * log(res1 / res2) * pow(sin((res1 + res2) * M_PI / 10), 2) + 0.8 * log(res2 / res1) * pow(cos((res1 + res2) * M_PI / 10), 2),1.5));

    cout << "\nresult = " << result;

}
